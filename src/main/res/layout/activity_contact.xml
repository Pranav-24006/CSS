<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#121212"
    tools:context=".ContactActivity">

  <ImageView
      android:id="@+id/imageView6"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:scaleType="centerCrop"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/back2"
      tools:layout_editor_absoluteX="0dp" />

  <ImageView
      android:id="@+id/imageView7"
      android:layout_width="100dp"
      android:layout_height="100dp"
      android:layout_margin="16dp"
      android:contentDescription="CSS Logo"
      android:onClick="home"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/css" />

  <ImageView
      android:id="@+id/imageView8"
      android:layout_width="100dp"
      android:layout_height="100dp"
      android:layout_margin="16dp"
      android:contentDescription="NIT DGP Logo"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/nitdgp_logo" />

  <Button
      android:id="@+id/button6"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_margin="16dp"
      android:backgroundTint="#8BA0D3"
      android:onClick="prev"
      android:text="About Us"
      android:textColor="#FFF"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toStartOf="@+id/button7"
      app:layout_constraintStart_toStartOf="parent" />

  <Button
      android:id="@+id/button7"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_margin="16dp"
      android:backgroundTint="#8BA0D3"
      android:onClick="home"
      android:text="Back to Start"
      android:textColor="#FFF"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/button6" />
  <ImageView
      android:id="@+id/pic1"
      android:layout_width="412dp"
      android:layout_height="wrap_content"
      android:contentDescription="Image"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/presi"
      tools:ignore="DuplicateIds" />

  <ImageView
      android:id="@+id/pic2"
      android:layout_width="412dp"
      android:layout_height="wrap_content"
      android:contentDescription="Image"
      android:alpha="0"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/presi"
      tools:ignore="DuplicateIds" />

  <ImageView
      android:id="@+id/pic3"
      android:layout_width="412dp"
      android:layout_height="wrap_content"
      android:contentDescription="Image"
      android:alpha="0"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/img3"
      tools:ignore="DuplicateIds" />

  <ImageView
      android:id="@+id/pic4"
      android:layout_width="412dp"
      android:layout_height="wrap_content"
      android:contentDescription="Image"
      android:alpha="0"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/img1"
      tools:ignore="DuplicateIds" />
  <ImageView
      android:id="@+id/pic5"
      android:layout_width="412dp"
      android:layout_height="wrap_content"
      android:contentDescription="Image"
      android:alpha="0"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/img1"
      tools:ignore="DuplicateIds" />
  <ImageView
      android:id="@+id/pic6"
      android:layout_width="412dp"
      android:layout_height="wrap_content"
      android:contentDescription="Image"
      android:alpha="0"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/img1"
      tools:ignore="DuplicateIds" />
  <ImageView
      android:id="@+id/pic7"
      android:layout_width="412dp"
      android:layout_height="wrap_content"
      android:contentDescription="Image"
      android:alpha="0"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/img1"
      tools:ignore="DuplicateIds" />

  <ImageView
      android:id="@+id/pic8"
      android:layout_width="412dp"
      android:layout_height="wrap_content"
      android:alpha="0"
      android:contentDescription="Image"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="1.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/img1"
      tools:ignore="DuplicateIds" />

  <ImageButton
      android:id="@+id/imageButton"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:contentDescription="next"
      android:onClick="nextpic"
      app:layout_constraintBottom_toTopOf="@+id/button7"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView8"
      app:srcCompat="@android:drawable/ic_media_next" />

  <ImageButton
      android:id="@+id/imageButton2"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:contentDescription="previous"
      android:onClick="prevpic"
      app:layout_constraintBottom_toTopOf="@+id/button6"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView7"
      app:srcCompat="@android:drawable/ic_media_previous" />

  <TextView
      android:id="@+id/textView5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginBottom="40dp"
      android:text="TextView"
      app:layout_constraintBottom_toTopOf="@+id/button7"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.498"
      app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
